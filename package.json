  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "typechain": "^8.1.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "@polkadot/api": "^9.10.5",
    "bip32": "^3.1.0",
    "bip39": "^3.0.4",
    "bip65": "^1.0.3",
    "bitcoinjs-lib": "^6.1.0",
    "dotenv": "^16.0.3",
    "ecpair": "^2.1.0",
    "hardhat": "^2.12.0",
    "superagent": "^8.0.6",
    "tiny-secp256k1": "^2.2.1"
  }
}
